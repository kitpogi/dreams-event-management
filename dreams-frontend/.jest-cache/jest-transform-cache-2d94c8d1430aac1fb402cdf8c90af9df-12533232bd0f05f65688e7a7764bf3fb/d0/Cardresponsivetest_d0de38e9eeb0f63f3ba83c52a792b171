"use strict";

var _react = require("@testing-library/react");
var _Card = require("../Card");
var _testHelpers = require("../../../test-utils/testHelpers");
var _jsxRuntime = require("react/jsx-runtime");
describe('Card Responsive Design', () => {
  beforeEach(() => {
    // Reset viewport before each test
    (0, _testHelpers.setViewport)(_testHelpers.VIEWPORTS.desktop.width, _testHelpers.VIEWPORTS.desktop.height);
  });
  it('renders correctly on mobile', () => {
    (0, _testHelpers.setViewport)(_testHelpers.VIEWPORTS.mobile.width, _testHelpers.VIEWPORTS.mobile.height);
    const {
      container
    } = (0, _react.render)(/*#__PURE__*/(0, _jsxRuntime.jsx)(_Card.Card, {
      children: "Mobile Card"
    }));
    expect(_react.screen.getByText('Mobile Card')).toBeInTheDocument();
    const card = container.firstChild;
    expect(card).toBeInTheDocument();
  });
  it('renders correctly on tablet', () => {
    (0, _testHelpers.setViewport)(_testHelpers.VIEWPORTS.tablet.width, _testHelpers.VIEWPORTS.tablet.height);
    const {
      container
    } = (0, _react.render)(/*#__PURE__*/(0, _jsxRuntime.jsx)(_Card.Card, {
      children: "Tablet Card"
    }));
    expect(_react.screen.getByText('Tablet Card')).toBeInTheDocument();
  });
  it('renders correctly on desktop', () => {
    (0, _testHelpers.setViewport)(_testHelpers.VIEWPORTS.desktop.width, _testHelpers.VIEWPORTS.desktop.height);
    const {
      container
    } = (0, _react.render)(/*#__PURE__*/(0, _jsxRuntime.jsx)(_Card.Card, {
      children: "Desktop Card"
    }));
    expect(_react.screen.getByText('Desktop Card')).toBeInTheDocument();
  });
  it('adapts to viewport changes', () => {
    const {
      container,
      rerender
    } = (0, _react.render)(/*#__PURE__*/(0, _jsxRuntime.jsx)(_Card.Card, {
      children: "Responsive Card"
    }));

    // Start with desktop
    (0, _testHelpers.setViewport)(_testHelpers.VIEWPORTS.desktop.width, _testHelpers.VIEWPORTS.desktop.height);
    expect(_react.screen.getByText('Responsive Card')).toBeInTheDocument();

    // Switch to mobile
    (0, _testHelpers.setViewport)(_testHelpers.VIEWPORTS.mobile.width, _testHelpers.VIEWPORTS.mobile.height);
    rerender(/*#__PURE__*/(0, _jsxRuntime.jsx)(_Card.Card, {
      children: "Responsive Card"
    }));
    expect(_react.screen.getByText('Responsive Card')).toBeInTheDocument();
  });
});
