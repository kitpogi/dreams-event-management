"use strict";

var _react = require("@testing-library/react");
var _badge = require("../badge");
var _testHelpers = require("../../../test-utils/testHelpers");
var _jsxRuntime = require("react/jsx-runtime");
describe('Badge Component', () => {
  it('renders badge with text', () => {
    (0, _react.render)(/*#__PURE__*/(0, _jsxRuntime.jsx)(_badge.Badge, {
      children: "Test Badge"
    }));
    expect(_react.screen.getByText('Test Badge')).toBeInTheDocument();
  });
  it('applies default variant', () => {
    const {
      container
    } = (0, _react.render)(/*#__PURE__*/(0, _jsxRuntime.jsx)(_badge.Badge, {
      children: "Default"
    }));
    const badge = container.firstChild;
    expect(badge).toHaveClass('bg-primary', 'text-primary-foreground');
  });
  it('applies secondary variant', () => {
    const {
      container
    } = (0, _react.render)(/*#__PURE__*/(0, _jsxRuntime.jsx)(_badge.Badge, {
      variant: "secondary",
      children: "Secondary"
    }));
    const badge = container.firstChild;
    expect(badge).toHaveClass('bg-secondary', 'text-secondary-foreground');
  });
  it('applies destructive variant', () => {
    const {
      container
    } = (0, _react.render)(/*#__PURE__*/(0, _jsxRuntime.jsx)(_badge.Badge, {
      variant: "destructive",
      children: "Destructive"
    }));
    const badge = container.firstChild;
    expect(badge).toHaveClass('bg-destructive', 'text-destructive-foreground');
  });
  it('applies outline variant', () => {
    const {
      container
    } = (0, _react.render)(/*#__PURE__*/(0, _jsxRuntime.jsx)(_badge.Badge, {
      variant: "outline",
      children: "Outline"
    }));
    const badge = container.firstChild;
    expect(badge).toHaveClass('text-foreground');
  });
  it('applies custom className', () => {
    const {
      container
    } = (0, _react.render)(/*#__PURE__*/(0, _jsxRuntime.jsx)(_badge.Badge, {
      className: "custom-class",
      children: "Custom"
    }));
    const badge = container.firstChild;
    expect(badge).toHaveClass('custom-class');
  });
  it('renders correctly in dark mode', () => {
    const {
      container
    } = (0, _testHelpers.renderDarkMode)(/*#__PURE__*/(0, _jsxRuntime.jsx)(_badge.Badge, {
      children: "Dark Badge"
    }));
    expect(_react.screen.getByText('Dark Badge')).toBeInTheDocument();
    const badge = container.firstChild;
    expect(badge).toHaveClass('bg-primary', 'text-primary-foreground');
  });
  it('renders correctly in light mode', () => {
    const {
      container
    } = (0, _testHelpers.renderLightMode)(/*#__PURE__*/(0, _jsxRuntime.jsx)(_badge.Badge, {
      children: "Light Badge"
    }));
    expect(_react.screen.getByText('Light Badge')).toBeInTheDocument();
    const badge = container.firstChild;
    expect(badge).toHaveClass('bg-primary', 'text-primary-foreground');
  });
  it('has proper accessibility attributes', () => {
    (0, _react.render)(/*#__PURE__*/(0, _jsxRuntime.jsx)(_badge.Badge, {
      "aria-label": "Status badge",
      children: "Active"
    }));
    const badge = _react.screen.getByText('Active');
    expect(badge).toHaveAttribute('aria-label', 'Status badge');
  });
});
